@model app_web_backend_Mediquei.Models.Familiar

@{
    ViewData["Title"] = "Details";
}

<h3>Detalhes do Familiar</h3>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Usuario)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Usuario.EMail)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" button type="button" class="btn btn-primary">Editar</a>
    <a href=/Familiares type="button" class="btn btn-secondary">Voltar</a>
